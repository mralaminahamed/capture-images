{
  "name": "capture-images",
  "version": "1.0.0",
  "description": "Capture all images from website page",
  "scripts": {
    "build": "webpack",
    "build:chrome": "webpack --env target=chrome",
    "build:trace": "node --trace-deprecation ./node_modules/.bin/webpack",
    "lint": "web-ext lint",
    "package": "npm run update:version && npm run build && web-ext build --overwrite-dest -a dist-firefox-package",
    "package:chrome": "npm run update:version && npm run build:chrome && web-ext build --overwrite-dest -s dist-chrome -a dist-chrome-package",
    "start": "webpack && web-ext run",
    "watch": "webpack -w",
    "watch:chrome": "webpack -w --env target=chrome",
    "update:addon": "node ./src/utils/update-addon",
    "update:version": "node ./src/utils/sync-app-version",
    "remove:orphan": "rm ./dist-firefox/assets/js/manifest.json.js",
    "zip-src": "node ./src/utils/zip-src",
    "packages:outdated": "npm outdated",
    "packages:upgrade": "npx npm-check-updates -u --timeout 60000 && npm install"
  },
  "keywords": [
    "images",
    "capture",
    "image download",
    "bulk image download",
    "capture images"
  ],
  "author": "Al-Amin Ahamed",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/mralaminahamed/capture-images.git"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/file-saver": "^2.0.3",
    "@types/sass": "^1.16.1",
    "@types/webextension-polyfill": "^0.8.0",
    "autoprefixer": "^10.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.3.0",
    "npm-check-updates": "^11.8.5",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.41.1",
    "sass-loader": "^12.1.0",
    "semver-parser": "^3.1.1",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "unused-files-webpack-plugin": "^3.4.0",
    "web-ext-plugin": "^1.3.3",
    "web-ext-types": "^3.2.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-preprocessor": "^0.1.12",
    "webpack-preprocessor-loader": "^1.1.4",
    "webpack-remove-empty-scripts": "^0.7.1"
  },
  "webExt": {
    "sourceDir": "dist-firefox"
  },
  "dependencies": {
    "@types/semver": "^7.3.8",
    "@types/semver-utils": "^1.1.1",
    "archiver": "^5.3.0",
    "cssnano": "^5.0.8",
    "file-saver": "^2.0.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "semver-utils": "^1.1.4",
    "web-ext": "^6.4.0",
    "webrtc-adapter": "^8.1.0"
  }
}
